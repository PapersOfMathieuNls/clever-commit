keywords = {Functional programming,Grammars,Source transformation,Term rewriting},
month = {aug},
number = {3},
pages = {190--210},
title = {{The TXL source transformation language}},
url = {http://www.sciencedirect.com/science/article/pii/S0167642306000669},
volume = {61},
year = {2006}
